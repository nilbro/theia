{
    "compilerOptions": {
        "downlevelIteration": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "skipLibCheck": true,
        "lib": [
            "es6",
            "dom"
        ],
        "module": "commonjs",
        "moduleResolution": "node",
        "noUnusedLocals": true,
        "outDir": "generators",
        "rootDir": "src",
        "sourceMap": true,
        "strict": true,
        "strictPropertyInitialization": false,
        "target": "es2015",
    },
    "include": [
        "src"
    ]
}